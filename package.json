{
  "name": "nx-react-native-cli",
  "version": "1.0.12",
  "description": "A react native starter (with NX) cli script",
  "type": "module",
  "files": [
    "lib",
    "templates",
    "README.md",
    "package.json"
  ],
  "bin": {
    "nxrn": "./lib/index.cjs"
  },
  "scripts": {
    "start": "./src/index.js",
    "build": "rm -rf lib && esbuild src/index.js --format=cjs --platform=node --bundle --outfile=lib/index.cjs --minify --analyze",
    "prepublish": "npm run build",
    "example": "npx nxrn create example",
    "relink": "npm unlink nxrn && npm run prepublish && npm link nxrn",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "email": "mercadojee@gmail.com",
    "name": "Jee Lorenz Mercado"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^10.0.2",
    "ora": "^8.0.1",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "esbuild": "^0.23.0"
  }
}
